{
    "name": "indigophp/fuel-doctrine",
    "type": "fuel-package",
    "description": "Fuel Doctrine 2 integration",
    "license": "MIT",
    "keywords": ["fuel", "doctrine2"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/fuel-core": "dev-develop",
        "indigophp/fuel-dbal": "dev-develop",
        "indigophp/doctrine-extensions": "dev-develop",
        "doctrine/orm": "~2.4",
        "gedmo/doctrine-extensions": "~2.3"
    },
    "require-dev": {
        "codeception/codeception": "~2.0.0",
        "codeception/mockery-module": "dev-master",
        "indigophp/codeception-fuel-module": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Fuel\\Doctrine\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "doctrine"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
